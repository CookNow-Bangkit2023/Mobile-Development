<!-- oval_shape.xml -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- Warna latar belakang -->
    <solid android:color="#EFF1F8" />

    <!-- Warna garis tepi (stroke) dan lebarnya -->
    <stroke
        android:width="2dp"
        android:color="#FFFF" /> <!-- Ganti warna sesuai kebutuhan -->

    <!-- Menentukan sudut bulat pada elemen -->
    <corners android:radius="50dp" /> <!-- Menggunakan radius setengah dari dimensi yang lebih besar -->

</shape>
